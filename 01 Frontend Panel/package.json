{
  "name": "01-frontend-panel",
  "version": "1.0.0",
  "description": "This is Frontend Panel",
  "browserslist": "cover 95%",
  "license": "ISC",
  "author": "zawlinn-vlog",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "server": "live-server",
    "compile:css": "sass ./assets/scss/main.scss ./assets/css/main.css -w",
    "prefix:css": "postcss ./assets/css/main.css -o ./assets/css/main.prefix.css --use autoprefixer",
    "start": "npm-run-all --parallel compile:css server"
  },
  "dependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "autoprefixer": "^10.4.21",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "sass": "^1.89.2"
  }
}
